{
  "name": "clay-kiln",
  "version": "8.17.0",
  "description": "Editor tools for Clay",
  "template": "template.handlebars",
  "scripts": {
    "lint": "eslint --quiet --ext .js,.vue edit.js view.js view-public.js directives inputs lib test",
    "lint:warnings": "eslint --ext .js,.vue edit.js view.js view-public.js directives inputs lib test",
    "lint:fix": "eslint --fix --ext .js,.vue edit.js view.js view-public.js directives inputs lib test",
    "lint:stats": "eslint --format node_modules/eslint-stats/byErrorandWarning.js --ext .js,.vue edit.js view.js view-public.js directives inputs lib test",
    "jest": "jest",
    "test": "npm run lint && BABEL_ENV=test npm run jest -- --coverage --no-cache",
    "test-local": "BABEL_ENV=test npm run jest -- --watch",
    "build": "BABEL_ENV=build webpack",
    "watch": "BABEL_ENV=build webpack -w",
    "prepublishOnly": "BABEL_ENV=build webpack -p && npm run generate-actions",
    "generate-actions": "jsdoc2md --files lib/**/actions.js > docs/vuex-actions.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clay/clay-kiln.git"
  },
  "keywords": [
    "clay",
    "kiln",
    "editor"
  ],
  "author": "New York Media",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clay/clay-kiln/issues"
  },
  "main": "model.js",
  "files": [
    "dist",
    "media",
    "template.handlebars",
    "schema.yml",
    "bootstrap.yml",
    "model.js"
  ],
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "browser": true,
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.{js,vue}",
      "<rootDir>/inputs/**/*.{js,vue}",
      "!<rootDir>/lib/utils/api.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "keen/(.*)$": "<rootDir>/node_modules/keen-ui/src/$1.vue"
    },
    "setupFiles": [
      "<rootDir>/test/setupFile.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    },
    "testURL": "http://domain.com/_pages/test-page.html?edit=true",
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!keen-ui)"
    ]
  },
  "homepage": "https://github.com/clay/clay-kiln",
  "devDependencies": {
    "@nymag/cid": "^1.0.1",
    "@nymag/dom": "^1.3.1",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "caret-position": "0.0.1",
    "chrono-node": "^1.4.6",
    "clay-log": "^1.4.2",
    "clayhandlebars": "github:clay/handlebars#upgrade-handlebars",
    "clayutils": "^2.7.1",
    "codemirror": "^5.54.0",
    "coveralls": "^3.1.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "csvjson": "^4.3.4",
    "cuid": "^1.3.8",
    "date-fns": "^1.30.1",
    "delegate": "^3.1.2",
    "element-client-rect": "^1.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-stats": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fuse.js": "^3.6.1",
    "hash-sum": "^1.0.2",
    "he": "^1.2.0",
    "html-tags": "^2.0.0",
    "intersection-observer": "^0.5.1",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdoc-to-markdown": "^3.0.1",
    "keen-ui": "^1.3.0",
    "keycode": "^2.1.8",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.0",
    "node-quill-converter": "^0.3.3",
    "node-sass": "^4.14.1",
    "nouislider": "^12.1.0",
    "nprogress": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^3.2.1",
    "path-to-regexp": "^3.2.0",
    "postcss-loader": "^2.1.6",
    "pre-commit": "^1.2.2",
    "promise-queue": "^2.2.3",
    "quill": "^1.3.7",
    "quill-delta": "^3.6.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.7.1",
    "routable": "0.0.5",
    "sanitize-html": "^1.24.0",
    "sass-loader": "^6.0.6",
    "set-dom": "^7.4.5",
    "snarkdown": "^1.2.2",
    "sortablejs": "^1.10.2",
    "speakingurl": "^14.0.1",
    "striptags": "^3.1.0",
    "style-loader": "^0.19.0",
    "url-parse": "^1.4.7",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.11",
    "vue-async-computed": "^3.8.2",
    "vue-avatar": "^2.3.0",
    "vue-click-outside": "^1.1.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.7.3",
    "vue-nprogress": "^0.1.5",
    "vue-observe-visibility": "^0.3.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.4.0",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {
    "amphora-search": "^6.2.0"
  }
}
