{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"],
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs", "transform-object-rest-spread", "istanbul"]
    },
    "build": {
      "plugins": ["transform-object-rest-spread", "lodash"]
    }
  }
}
